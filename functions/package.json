    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^4.9.0"
  },
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": ["@biomejs/biome", "protobufjs"]
  }
}
